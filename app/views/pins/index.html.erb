<div id="pins" class="transitions-enabled">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
      <%= link_to (image_tag pin.image.url), pin%>
      <div class="panel-body">
        <h2><%= link_to pin.title, pin %></h2>
        <p class="user">Submitted by: <%= pin.user.email%></p>
      </div>
    </div>
  <% end %>
</div>
